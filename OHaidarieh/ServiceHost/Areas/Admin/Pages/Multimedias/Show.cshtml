@model List<Haidarieh.Application.Contracts.Multimedia.MultimediaViewModel>
@{
}
<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
    <h4 class="modal-title">سوابق مراسم</h4>
</div>
<div class="row">
    <div class="col-md-12">
        <div class="panel panel-default">
            @*<div class="panel-heading">
                    <h3 class="panel-title">@Model فایلهای</h3>
                </div>*@
            <div class="panel-body">
                <div class="row">
                    <div class="col-md-4 col-sm-4 col-xs-4">
                        <div class="table table-striped table-bordered">
                            @foreach (var item in Model)
                            {
                                <div>
                                    <img src="~/@item.FileAddress" style="width: 100px; height: 100px" />
                                </div>
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal-footer">
    <button type="button" class="btn btn-default waves-effect" data-dismiss="modal">بستن</button>
</div>

